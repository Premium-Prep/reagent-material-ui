    "karma-cljs-test": "^0.1.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rimraf": "^3.0.2",
    "shadow-cljs": "^2.11.6",
    "terser-webpack-plugin": "^4.2.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^5.3.0"
  },
  "dependencies": {
    "@emotion/core": "^10.1.0",
    "@emotion/styled": "^10.0.27",
    "@material-ui/core": "5.0.0-alpha.14",
    "@material-ui/lab": "5.0.0-alpha.14",
